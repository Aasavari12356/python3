#comparison

#conditional
#if,else

#pgm1
numT=50
if numT > 0 and numT <= 5:
    print("10 % discount")
if numT > 5 and numT <= 10:
    print("20 % discount")
if numT > 10:
    print("30 % discount")


#pgm2
numT = -20
if  numT > 0 and numT <= 5:
    print("10 % discount")
elif  numT > 5 and numT <= 10:
    print("20 % discount")
elif numT > 10:
    print("30 % discount")
else:
    print("incorrect input")

#pgm3
    
numT=7
if numT > 0 and numT <5:
    print("10 % discount")
if numT > 5 and numT < 10:
    print("20 % discount")
if numT > 10:
    print("30 % discount")

#pgm4
s= 10
t=5
if s > t:
    print("s is greater")
else:
    print ("t is a greater")

#pgm5
x1=50
x2=100
x3=300
if x1 > x2 and x1 > x3:
    print("x1 is a greater")
if x2 > x3 and x2 > x1:
    print("x2 is a greater")
if x3 > x1 and x3 > x1:
    print("x3 is a greater")

#pgm6
age=18
e="can drive" if age >= 18 else "can't drive"
print(e)

