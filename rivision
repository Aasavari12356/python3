#2 Arithmatic
x=5
Y=10
print(x+Y)
print(x-Y)
print(x*Y)
print(x/Y)
print(x%Y)

#3 def 
def Cal(x,y):
    print(x+Y)
    print(x-Y)
    print(x*Y)
    print(x/Y)
    print(x%Y)
Cal(10,5)
Cal(50,5)

#4 in def type
##1
def addA():
    print(9+9)
addA()
addA()    

##2
def addB(x,y):
    print(x+y)
addB(9,3)
addB(10,5)    

##3
def addC(x,y):
    return x+y
q=addC(9,3)
print(q)
print(q+q)


